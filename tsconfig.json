{
  "compilerOptions": {
    "target": "ES2022",
    "rootDir": "src",
    "outDir": "./dist",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": ["src", "node_modules/@types"]
}
